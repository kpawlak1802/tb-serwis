---
interface Props {
	title: string;
}

const { title }:Props = Astro.props as Props;
---

<!doctype html>
<html lang="pl">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="TB serwis - usługi WUKO i kamerowania" />
    <meta name="keywords" content="TB serwis, wuko, kamerowanie, udrażnianie kanalizacji i zewnętrznych instalacji sanitarnych oraz deszczowych, czyszczenie drenaży, płukanie rurociągów i zbiorników, usuwanie osadów z sieci" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/images/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
		<slot />
	</body>
</html>